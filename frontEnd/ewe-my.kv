#:kivy 1.9.1

<CustomButton@Button>:
	size_hint: [None, None]
	size: [100, 100]
<GridL>
	rows: 3
	spacing: 40
	padding: 40
	AnchorLayout:
		anchor_x: 'left'
		anchor_y: 'top'
		canvas:
			Color:
				rgb: [1, 1, 1]
			Rectangle
				pos: self.pos
				size: self.size
		CustomButton:
			text: 'Are'
	AnchorLayout:
		anchor_x: 'center'
		anchor_y: 'top'
		canvas:
			Color:
				rgb: [0, 1, 1]
			Rectangle
				pos: self.pos
				size: self.size
		CustomButton:
			text: 'you'
	AnchorLayout:
		anchor_x: 'right'
		anchor_y: 'top'
		canvas:
			Color:
				rgb: [1, 0, 1]
			Rectangle
				pos: self.pos
				size: self.size
		CustomButton:
			text: 'feeling'
	AnchorLayout:
		anchor_x: 'left'
		anchor_y: 'center'
		canvas:
			Color:
				rgb: [1, 1, 0]
			Rectangle
				pos: self.pos
				size: self.size
		CustomButton:
			text: 'it'
	AnchorLayout:
		anchor_x: 'center'
		anchor_y: 'center'
		canvas:
			Color:
				rgb: [0, 0, 1]
			Rectangle
				pos: self.pos
				size: self.size
		CustomButton:
			text: 'now'
	AnchorLayout:
		anchor_x: 'right'
		anchor_y: 'center'
		canvas:
			Color:
				rgb: [0, 1, 0]
			Rectangle
				pos: self.pos
				size: self.size
		CustomButton:
			text: 'Mr.'
	AnchorLayout:
		anchor_x: 'left'
		anchor_y: 'bottom'
		canvas:
			Color:
				rgb: [1, 0, 0]
			Rectangle
				pos: self.pos
				size: self.size
		CustomButton:
			text: 'Krabs'
	AnchorLayout:
		anchor_x: 'center'
		anchor_y: 'bottom'
		canvas:
			Color:
				rgb: [0, 0, 0]
			Rectangle
				pos: self.pos
				size: self.size
		CustomButton:
			text: '???'
	AnchorLayout:
		anchor_x: 'right'
		anchor_y: 'bottom'
		canvas:
			Color:
				rgb: [1, 1, 1]
			Rectangle
				pos: self.pos
				size: self.size
		CustomButton:
			text: 'ewe'
